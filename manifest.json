{
    "name": "Zorin New Tab",
    "description": "Zorin New Tab!",
    "version": "1.0.0",
    "manifest_version": 3,
    "permissions": [
        "tabs",
        "storage",
        "scripting"
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "action": {
        "default_icon": {
            "16": "/icons/icon-16.png",
            "32": "/icons/icon-32.png",
            "64": "/icons/icon-64.png",
            "128": "/icons/icon-128.png"
        }
    },
    "icons": {
        "16": "/icons/icon-16.png",
        "32": "/icons/icon-32.png",
        "64": "/icons/icon-64.png",
        "128": "/icons/icon-128.png"
    }
}